
<h1>Listing Miscs</h1>

<table>
  <thead>
    <tr>
      <th>Misc date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @miscs.each do |misc| %>
      <tr>
        <td><%= misc.misc_date %></td>
        <td><%= misc.description %></td>
        <td><%= link_to 'Show', misc %></td>
        <td><%= link_to 'Edit', edit_misc_path(misc) %></td>
        <td><%= link_to 'Destroy', misc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Misc', new_misc_path %>
